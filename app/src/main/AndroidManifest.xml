<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eureton.warmaodds"
    android:versionCode="4"
    android:versionName="1.10"
    >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="23"
        />

    <application
        android:allowBackup="true"
        android:name=".android.Application"
        android:icon="@drawable/launcher_logo"
        android:label="@string/activities.main"
        android:theme="@style/Theme"
        >
        <activity
            android:name=".views.impl.MainActivity"
            android:label="@string/activities.main"
			android:theme="@style/Theme.Main"
			>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".views.impl.ModifiersActivity"
            android:label="@string/activities.modifiers"
            android:theme="@style/Theme.Modifiers"
            android:icon="@drawable/no_icon"
			>
        </activity>
    </application>

</manifest>
