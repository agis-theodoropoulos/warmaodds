<?xml version="1.0" encoding="utf-8"?>
<layer-list
	xmlns:android="http://schemas.android.com/apk/res/android"
	>
	<item>
		<shape
			android:shape="ring"
			android:thickness="16dp"
			android:innerRadius="0dp"
			android:useLevel="false"
			>
			<size
				android:width="@dimen/remove.ic.width"
				android:height="@dimen/remove.ic.height"
				/>
			<solid
				android:color="@color/remove.ic.ring"
				/>
		</shape>
	</item>
	<item
		android:drawable="@drawable/remove_red"
		>
	</item>
</layer-list>
